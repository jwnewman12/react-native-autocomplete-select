{
  "name": "react-native-autocomplete-select",
  "version": "1.1.0",
  "description": "AutoComplete with selection for react-native",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Vizir/react-native-autocomplete-select.git"
  },
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "precommit": "npm run lint && npm test",
    "test": "NODE_ENV=production jest"
  },
  "keywords": [
    "react-native",
    "autocomplete",
    "select"
  ],
  "author": "Thiago Porto",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-native": "~0.57.4",
    "string_score": "git+https://github.com/ThiagoNP/string_score.git"
  },
  "devDependencies": {
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^5.6.1",
    "eslint-plugin-react": "^7.11.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "^16.6.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "index.js",
      "AutoComplete/**/*.js"
    ],
    "coverageDirectory": "./coverage",
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native)"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
